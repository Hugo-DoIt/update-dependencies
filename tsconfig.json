{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",
  
    "compilerOptions": {
      "lib": ["ESNext"],
      "module": "CommonJS",
      "target": "ESNext",
  
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    }
  }